function g(o) {
        return document.getElementById(o);
    }
    function qh(n) {
        for(var i=0;i<=10;i++){
            g('ta_'+i).className='bb';
            g('ta_'+n).className='aa';
            g('tab_'+i).className='sub-nav hide-nav';
            g('tab_'+n).className='sub-nav show-nav';
            g('tab_0').className='hide-nav';
            g('tab_1').className='hide-nav';
            g('tab_6').className='hide-nav';
            g('tab_7').className='hide-nav';
            g('tab_8').className='hide-nav';
            g('tab_9').className='hide-nav';
            g('tab_10').className='hide-nav';
        }
    }
    function hideNav() {
        g('ta_0').className='bb';
        g('ta_1').className='bb';
        g('ta_6').className='bb';
        g('ta_7').className='bb';
        g('ta_8').className='bb';
        g('ta_9').className='bb';
        g('ta_10').className='bb';
    }
    function  showPic() {
        g('show-pic').className='show-pic2';
    }
    function hidePic() {
        g('show-pic').className='show-pic1';
    }
